{
  "manifest_version": 3,
  "name": "Taobao Image Downloader",
  "version": "1.0",
  "description": "타오바오 이미지를 다운로드 해주는 확장프로그램입니다. + 1688, Tmall, .etc",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["jszip.min.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
